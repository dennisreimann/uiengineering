{
  "version": "1.0.0",
  "name": "uiengineering",
  "license": "UNLICENSED",
  "repository": "git@github.com:dennisreimann/uiengineering.git",
  "description": "www.uiengineering.de",
  "engines": {
    "node": ">=7.1.0"
  },
  "config": {
    "deployment": {
      "command": "rsync -ravzulO --chmod=u=rwX,g=rX --delete-after",
      "target": "uiengine@uiengineering.de:html"
    }
  },
  "scripts": {
    "cleanup": "rm -rf dist sketch",
    "images": "gulp images",
    "sketch": "rm -rf sketch && html-sketchapp --config html-sketchapp.config.js && open -a Sketch",
    "start": "npm run cleanup && gulp develop --dev",
    "build": "npm run cleanup && gulp build",
    "prod": "npm run cleanup && gulp production --assetHost=cdn.uiengineering.de",
    "build-and-deploy": "npm run prod && npm run deploy",
    "deploy": "npm run deploy:assets && npm run deploy:site",
    "deploy:assets": "$npm_package_config_deployment_command --exclude-from=.rsync-exclude-assets dist/fonts dist/images dist/mp3s dist/styles $npm_package_config_deployment_target",
    "deploy:site": "$npm_package_config_deployment_command --exclude-from=.rsync-exclude-site dist/ $npm_package_config_deployment_target"
  },
  "devDependencies": {
    "@uiengine/core": "^0.19.3",
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "css-mqpacker": "^6.0.2",
    "csswring": "^6.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-banner": "^0.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-concat-json": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-mvb": "^0.6.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-resolve-dependencies": "^2.2.0",
    "gulp-rev-all": "^0.8.24",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-sitemap": "^5.1.0",
    "gulp-stylus": "^2.7.0",
    "gulp-theo": "^2.0.0-beta.1",
    "gulp-uglify": "^3.0.0",
    "html-sketchapp-cli": "0.5.0",
    "jstransformer-marked": "^1.0.3",
    "marked": "^0.3.19",
    "merge-stream": "^1.0.1",
    "moment": "^2.22.1",
    "pug": "^2.0.3",
    "run-sequence": "^2.2.1",
    "standard": "^11.0.1",
    "theo": "^7.0.1",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "amplitudejs": "^3.2.3"
  }
}
