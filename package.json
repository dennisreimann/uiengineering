{
  "version": "1.0.0",
  "name": "uiengineering",
  "license": "UNLICENSED",
  "repository": "git@github.com:dennisreimann/uiengineering.git",
  "description": "www.uiengineering.de",
  "engines": {
    "node": ">=7.1.0"
  },
  "config": {
    "deployment": {
      "command": "rsync -ravzulO --chmod=u=rwX,g=rX --delete-after",
      "target": "uiengine@uiengineering.de:html"
    }
  },
  "scripts": {
    "cleanup": "rm -rf dist",
    "images": "gulp images",
    "start": "yarn cleanup && gulp develop --dev",
    "build": "yarn cleanup && gulp build",
    "prod": "yarn cleanup && gulp production --assetHost=cdn.uiengineering.de",
    "build-and-deploy": "yarn prod && yarn deploy",
    "deploy": "yarn deploy:assets && yarn deploy:site",
    "deploy:assets": "$npm_package_config_deployment_command --exclude-from=.rsync-exclude-assets dist/fonts dist/images dist/mp3s dist/styles $npm_package_config_deployment_target",
    "deploy:site": "$npm_package_config_deployment_command --exclude-from=.rsync-exclude-site dist/ $npm_package_config_deployment_target"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.13",
    "coffee-script": "^1.12.7",
    "css-mqpacker": "^6.0.1",
    "csswring": "^6.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-banner": "^0.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-concat-json": "^1.0.0",
    "gulp-data": "^1.2.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-mvb": "^0.5.3",
    "gulp-newer": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-pug": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-resolve-dependencies": "^2.2.0",
    "gulp-rev-all": "^0.8.24",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-sitemap": "^4.2.0",
    "gulp-stylus": "^2.6.0",
    "gulp-theo": "^1.0.0-alpha.5",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jstransformer-marked": "^1.0.2",
    "marked": "^0.3.6",
    "merge-stream": "^1.0.1",
    "moment": "^2.18.1",
    "pug": "^2.0.0-rc.4",
    "run-sequence": "^2.2.0",
    "standard": "^10.0.3",
    "theo": "^6.0.0-beta.7",
    "uiengine": "^0.11.0-alpha.1",
    "yargs": "^9.0.1"
  }
}
